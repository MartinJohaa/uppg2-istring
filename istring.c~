/*
 * Returns a new istring instance created from supplied
 * string. Returns NULL if out of memory. Returns NULL string if
 * argument str is NULL.
 */
#include<stdio.h>
#include<stdlib.h>



char *istring_mk(const char* str){
  if (str == NULL){
    return NULL;
  }
  // char* str = "test";
  char temp[strlen(str) + 4];
  int length = strlen(str);
  else if(length < 10){
    temp[0] = 0;
    temp[1] = 0;
    temp[2] = 0;
    temp[3] = itoa(length,temp,10);
  }
  else if(length < 100 && length >= 10){
    temp[0] = 0;
    temp[1] = 0;
    temp[2] = itoa(length,temp,10);
  }
  else if(length < 1000 && length >= 100){
    temp[0] = 0;
    temp[1] = itoa(length,temp,10);
  }
  else if(length < 10000 && length >= 1000){
    temp[0] = itoa(length,temp,10);
  }
  printf ("%s\n", temp);
}


int main(int argc, char *argv[])
{
  isting_mk("hej");
  istring_mk("halalalalaalalalalalalalallalalalalalalla");
  istring_mk("");
  return 0;
}
